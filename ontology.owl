<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/ontology/"
     xml:base="http://example.org/ontology/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://example.org/ontology/">
        <rdfs:label>Example Ontology</rdfs:label>
        <rdfs:comment>An example ontology for use with OntoSpreadEd</rdfs:comment>
    </owl:Ontology>
    
    <!-- Example Class -->
    <owl:Class rdf:about="http://example.org/ontology/EX_0000001">
        <rdfs:label>Thing</rdfs:label>
        <rdfs:comment>The root class of the example ontology</rdfs:comment>
    </owl:Class>
    
    <!-- Example Property -->
    <owl:ObjectProperty rdf:about="http://example.org/ontology/EX_0000101">
        <rdfs:label>part of</rdfs:label>
        <rdfs:comment>A is part of B</rdfs:comment>
    </owl:ObjectProperty>
</rdf:RDF>